{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [],
  "enabledManagers": ["npm"],
  "includePaths": ["backend/catalog/package.json"],
  "ignoreDeps": ["*"],
  "packageRules": [
    {
      "matchPackageNames": ["jest", "@types/jest"],
      "ignoreDeps": [],
      "enabled": true,
      "recreateWhen": "always"
